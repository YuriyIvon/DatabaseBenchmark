    <PackageReference Include="MySqlConnector" Version="2.2.2" />
    <PackageReference Include="NEST" Version="7.17.5" />
    <PackageReference Include="Npgsql" Version="7.0.1" />
    <PackageReference Include="Octonica.ClickHouseClient" Version="2.2.9" />
    <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="3.21.80" />
    <PackageReference Include="SimpleInjector" Version="5.4.1" />
    <PackageReference Include="Snowflake.Data" Version="2.0.19" />
