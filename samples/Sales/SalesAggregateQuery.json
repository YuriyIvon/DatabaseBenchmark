{
  "Columns": [ "ItemType" ],
  "Condition": {
    "Operator": "And",
    "Conditions": [
      { "ColumnName": "Country", "Operator": "Equals", "RandomizeValue": true },
      { 
        "ColumnName": "OrderDate",
        "Operator": "GreaterEquals",
        "RandomizeValue": true,
        "ValueRandomizationRule": { "UseExistingValues": false, "MinDateTimeValue": "2015-01-01", "MaxDateTimeValue": "2016-01-01", "DateTimeValueStep": "7.00:00:00" }
      },
      { 
        "ColumnName": "OrderDate",
        "Operator": "Lower",
        "RandomizeValue": true,
        "ValueRandomizationRule": { "UseExistingValues": false, "MinDateTimeValue": "2017-01-01", "MaxDateTimeValue": "2018-01-01", "DateTimeValueStep": "7.00:00:00" }
      }
    ]
  },
  "Aggregate": {
    "GroupColumnNames": ["ItemType"],
    "ResultColumns": [
      { "Function": "Count", "ResultColumnName": "OrderCount" },
      { "SourceColumnName": "UnitsSold", "Function": "Sum", "ResultColumnName": "TotalUnitsSold" }
    ]
  },
  "Sort": [ { "ColumnName": "ItemType", "Direction": "Ascending" } ]
}