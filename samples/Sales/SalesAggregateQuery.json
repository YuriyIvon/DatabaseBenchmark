{
  "Columns": [
    "Country"
  ],
  "Condition": {
    "Operator": "And",
    "Conditions": [
      {
        "ColumnName": "ItemType",
        "Operator": "Equals",
        "RandomizeValue": true
      },
      {
        "ColumnName": "OrderDate",
        "Operator": "GreaterEquals",
        "Value": "2016-01-01"
      },
      {
        "ColumnName": "OrderDate",
        "Operator": "Lower",
        "Value": "2017-01-01"
      }
    ]
  },
  "Aggregate": {
    "GroupColumnNames": ["Country"],
    "ResultColumns": [
      {
        "Function": "Count",
        "ResultColumnName": "OrderCount"
      },
      {
        "SourceColumnName": "UnitsSold",
        "Function": "Sum",
        "ResultColumnName": "TotalUnitsSold"
      }
    ]
  },
  "Sort": [
    {
      "ColumnName": "Country",
      "Direction": "Ascending"
    }
  ]
}